<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@android:color/white"
	android:orientation="vertical"
	tools:context="com.iot.volunteer.activity.ActivityAgree"
	android:clickable="true">
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="@dimen/title_bar_height"
		android:background="@color/color_tab_selected">
		<ImageView
			android:id="@+id/ivBack"
			android:layout_width="@dimen/dim_icon_size"
			android:layout_height="@dimen/dim_icon_size"
			android:layout_centerVertical="true"
			android:layout_margin="@dimen/dim_gap_2"
			android:src="@drawable/ic_back_white"/>
		<TextView
			android:id="@+id/tvTitle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerInParent="true"
			android:textColor="@android:color/white"
			android:textSize="@dimen/dim_font_3"
			android:text="@string/str_progress_view"/>
	</RelativeLayout>

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding ="@dimen/dim_gap_4"
			android:orientation="vertical">
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_notification_time"/>
				<TextView
					android:id="@+id/tvNotificationTime"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_contact_aid_agency"/>
				<TextView
					android:id="@+id/tvContactAidAgency"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_phone_number"/>
				<TextView
					android:id="@+id/tvPhoneNumber"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_aid_task"/>
				<TextView
					android:id="@+id/tvTask"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_alarm"/>
				<TextView
					android:id="@+id/tvAlarmContent"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_device"/>
				<TextView
					android:id="@+id/tvDevice"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_serial_title"/>
				<TextView
					android:id="@+id/tvSerial"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_point"/>
				<TextView
					android:id="@+id/tvPoint"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_mission_start_time"/>
				<TextView
					android:id="@+id/tvMissionStartTime"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/dim_gap_3"
				android:padding="@dimen/dim_gap_2"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"
					android:text="@string/str_mission_end_time"/>
				<TextView
					android:id="@+id/tvMissionEndTime"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_alignParentRight="true"
					android:textColor="@android:color/black"
					android:textSize="@dimen/dim_font_2"/>
			</RelativeLayout>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/color_bg_even"/>
		</LinearLayout>
	</ScrollView>
</LinearLayout>